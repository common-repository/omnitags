        "params": [
          {
            "ID": "bing_webmaster_tools_verification_meta",
            "name": "HTML meta tag",
            "field_key": "bing_webmaster_tools_verification_meta",
            "field_type": "text",
            "placeholder": "<meta name=\"msvalidate.01\" content=\"XXXXXXXXXXXXXXXXXXXXXXXXXX\" />",
            "wp_hook": ";wp_head;"
          }
        ]
      }
    ]
  },
